{
  "name": "nestjs-mongodb-native",
  "version": "1.2.0",
  "description": "A package for integrating MongoDB with NestJS using the MongoDB native driver",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "prepare": "husky install",
    "clear:dist": "rimraf ./dist",
    "prepublish": "npm run test && npx --no-install lint-staged && npm run clear:dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Llirik1337/nestjs-mongodb-native.git"
  },
  "keywords": [
    "nestjs",
    "mongodb",
    "mongodb-native",
    "database",
    "driver"
  ],
  "bugs": {
    "url": "https://github.com/Llirik1337/nestjs-mongodb-native/issues"
  },
  "homepage": "https://github.com/Llirik1337/nestjs-mongodb-native#readme",
  "lint-staged": {
    "**/*.{js,ts,json}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "author": "Llirik1337",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/testing": "^10.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "mongodb-memory-server": "^8.16.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "@nestjs/common": "*",
    "@nestjs/core": "*",
    "mongodb": "^5.6.0",
    "reflect-metadata": "*",
    "rxjs": "*"
  }
}
